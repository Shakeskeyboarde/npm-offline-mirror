{
  "name": "npm-offline-mirror",
  "description": "Fast, safe, reproducible builds using NPM.",
  "author": "Chris Ackerman <chris@topher.land>",
  "license": "ISC",
  "version": "1.0.5",
  "files": [
    "lib/**"
  ],
  "directories": {
    "lib": "lib"
  },
  "type": "module",
  "bin": {
    "npm-offline-mirror": "lib/bin.js"
  },
  "sideEffects": false,
  "scripts": {
    "test": "del-cli out && rc test -o",
    "prepack": "del-cli lib *.tgz && tsc --project tsconfig.build.json && chmodx --package",
    "start": "npm run prepack && node lib/bin.js",
    "modernize": "npx npm-check-updates -i --format group"
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/jest": "^28.1.8",
    "@types/node": "~16.0.0",
    "chmodx": "^1.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.22.0",
    "eslint-config-rational": "^1.0.4",
    "jest": "^28.1.3",
    "parallel-rc": "^1.0.12",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ini": "^3.0.1"
  },
  "engines": {
    "npm": ">=7",
    "node": ">=16"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shakeskeyboarde/npm-offline-mirror.git"
  },
  "keywords": [
    "npm",
    "offline",
    "mirror",
    "cache",
    "shrinkwrap",
    "shrinkpack",
    "lock",
    "lockfile",
    "dependency",
    "dependencies",
    "package"
  ]
}
